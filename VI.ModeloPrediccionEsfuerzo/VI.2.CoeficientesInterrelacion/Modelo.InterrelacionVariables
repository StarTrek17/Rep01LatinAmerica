Modelo5 = lm(nwell1 ~ (indsct + orgtyp + apptyp + dvltyp + 
           dvlplt + lngtyp + prglng1 + fncsze + smmwrkeff + 
	   maxteasze + dbms1 + dvlmtd + loc), 
     datos = isbsg400numno)

